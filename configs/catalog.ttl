@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .

<http://netwerkdigitaalerfgoed.nl/catalogs/network-of-terms> a dcat:Catalog ;
    dcterms:title "Catalog of Network of Terms"@en ;
    dcat:dataset <https://data.rkd.nl/rkdartists>,
        <https://data.cultureelerfgoed.nl/term/id/cht>,
        <http://data.bibliotheken.nl/id/dataset/persons>,
        <https://data.netwerkdigitaalerfgoed.nl/NMVW/thesaurus>,
        <http://vocab.getty.edu/aat>,
        <https://www.wikidata.org> .

<https://data.rkd.nl/rkdartists> a dcat:Dataset ;
    dcterms:title "RKDartists"@en ;
    dcterms:identifier "rkdartists" ;
    dcat:distribution <https://data.netwerkdigitaalerfgoed.nl/rkd/rkdartists/sparql> .

<https://data.netwerkdigitaalerfgoed.nl/rkd/rkdartists/sparql> a dcat:Distribution ;
    dcterms:title "RKDartists as SPARQL"@en ;
    dcterms:identifier "rkdartists-sparql" ;
    dcat:accessService [
        dcat:endpointURL <https://api.data.netwerkdigitaalerfgoed.nl/datasets/rkd/rkdartists/services/rkdartists/sparql> ;
        dcat:servesDataset <https://data.rkd.nl/rkdartists> ;
        schema:potentialAction [
            a schema:SearchAction ;
            schema:query "file://queries/rkdartists.rq"
        ]
    ] .

