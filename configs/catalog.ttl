<https://data.cultureelerfgoed.nl/cht/hdt> a dcat:Distribution ;
    dcterms:title "CHT as HDT file"@en ;
    dcterms:identifier "cht-hdt" ;
    dcat:accessService [
        dcterms:type <http://netwerkdigitaalerfgoed.nl/ontologies/accessServiceTypes/hdt> ;
        dcat:endpointURL <data/cht.hdt> ;
        dcat:servesDataset <https://data.cultureelerfgoed.nl/term/id/cht> ;
        schema:potentialAction [
            a schema:SearchAction ;
            schema:query """
                PREFIX skos: <http://www.w3.org/2004/02/skos/core#>

                CONSTRUCT {
                    ?uri a skos:Concept ;
                    skos:prefLabel ?prefLabel ;
                }
                WHERE {
                    ?uri skos:prefLabel ?prefLabel
                    FILTER(LANG(?prefLabel) = "nl")
                    FILTER(CONTAINS(LCASE(?prefLabel), LCASE(?searchTerm)))
                }
                LIMIT 50
            """
        ]
    ] .

