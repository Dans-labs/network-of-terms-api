<https://data.cultureelerfgoed.nl/term/id/cht> a dcat:Dataset ;
    dcterms:title "Cultuurhistorische Thesaurus"@en ;
    dcterms:identifier "cht" ;
    dcat:distribution <https://data.cultureelerfgoed.nl/PoolParty/sparql/term/id/cht/2>,
        <https://data.cultureelerfgoed.nl/cht/hdt> .

<https://data.cultureelerfgoed.nl/PoolParty/sparql/term/id/cht/2> a dcat:Distribution ;
    dcterms:title "CHT as SPARQL"@en ;
    dcterms:identifier "cht-sparql" ;
    dcat:accessService [
        dcterms:type <http://netwerkdigitaalerfgoed.nl/ontologies/accessServiceTypes/sparql> ;
        dcat:endpointURL <https://data.cultureelerfgoed.nl/PoolParty/sparql/term/id/cht> ;
        dcat:servesDataset <https://data.cultureelerfgoed.nl/term/id/cht> ;
        schema:potentialAction [
            a schema:SearchAction ;
            schema:query "file://queries/cht.rq"
        ]
